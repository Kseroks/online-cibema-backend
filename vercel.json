{
    "version": 2,
    "env": {
        "MONGO_URI": "@online_cinema_mongo_uri",
        "NODE_ENV": "@online_cinema_node",
        "JWT_SECRET": "@online_cinema_jwt",
        "PORT": "@online_cinema_port",
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}
